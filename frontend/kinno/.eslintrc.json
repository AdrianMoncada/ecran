{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"next",
		"plugin:prettier/recommended",
		"next/core-web-vitals"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [ "prettier"],
	"rules": {
		"indent": "off",
		// "linebreak-style": [
		// 	"error",
		// 	"unix"
		// ],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"react/prop-types": 0
	}
}
